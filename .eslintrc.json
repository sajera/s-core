{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "globals": {
        "console": true
    },
    "rules": {
        "strict": ["warn"],
        "no-undef": ["warn"],
        "curly": ["warn", "all"],
        "valid-jsdoc": ["warn", {
            "requireReturn": false,
            "requireReturnType": true,
            "requireParamDescription": false,
            "requireReturnDescription": false,
            "preferType": {
                "boolean": "Boolean",
                "number": "Number",
                "object": "Object",
                "string": "String"
            }
        }],
        "require-jsdoc": ["warn", {"require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true,
            "ArrowFunctionExpression": true,
            "FunctionExpression": true
        }}],
        "radix": ["warn", "as-needed"],
        "no-duplicate-imports": ["warn"],
        "no-console":["warn", { "allow": ["warn", "error"]}],
        "semi-spacing": ["warn", {"before": false, "after": true}],
        "no-fallthrough": ["warn", { "commentPattern": "no-break" }],
        "no-extend-native": ["warn", { "exceptions": ["Error", "Array"] }],
        "no-multiple-empty-lines": ["warn", { "max": 2, "maxEOF": 1, "maxBOF": 1}],
        "no-restricted-globals": ["warn", { "name": "event", "message": "Use local parameter instead." }],
        "no-throw-literal": ["warn"],
        "no-use-before-define": ["warn"],
        "no-undef-init": ["warn"],
        "no-useless-computed-key": ["warn"],
        "no-unneeded-ternary": ["warn", { "defaultAssignment": true }],
        "no-unsafe-negation": ["warn"],
        "no-unused-vars": ["warn"],
        "arrow-spacing": ["warn", { "before": true, "after": true }],
        "valid-typeof": ["error", { "requireStringLiterals": true }],
        "comma-dangle": ["warn", "never"],
        "comma-spacing": ["warn", { "before": false, "after": true }],
        "comma-style": ["warn", "last"],
        "constructor-super": ["warn"],
        "default-case": ["warn"],
        "dot-location": ["warn", "property"],
        "func-call-spacing": ["warn", "never"],
        "key-spacing": ["warn", { "afterColon": true }],
        "operator-linebreak": ["warn", "before"],
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "no-eval": ["error"],
        "no-alert": ["error"],
        "no-empty": ["error"],
        "no-octal": ["error"],
        "use-isnan": ["error"],
        "no-caller": ["error"],
        "no-eq-null": ["error"],
        "no-bitwise": ["error"],
        "no-debugger": ["error"],
        "no-ex-assign": ["error"],
        "no-dupe-args": ["error"],
        "no-dupe-keys": ["error"],
        "no-obj-calls": ["error"],
        "no-redeclare": ["error"],
        "no-delete-var": ["error"],
        "no-extra-semi": ["error"],
        "no-extra-bind": ["error"],
        "no-new-symbol": ["error"],
        "no-cond-assign": ["error"],
        "no-func-assign": ["error"],
        "no-extra-label": ["error"],
        "no-process-env": ["error"],
        "no-self-assign": ["error"],
        "no-unreachable": ["error"],
        "no-class-assign": ["error"],
        "no-catch-shadow": ["error"],
        "no-regex-spaces": ["error"],
        "no-const-assign": ["error"],
        "no-control-regex": ["error"],
        "no-sparse-arrays": ["error"],
        "no-empty-pattern": ["error"],
        "no-duplicate-case": ["error"],
        "no-invalid-regexp": ["error"],
        "no-negated-in-lhs": ["error"],
        "no-unsafe-finally": ["error"],
        "no-native-reassign": ["error"],
        "no-this-before-super": ["error"],
        "no-case-declarations": ["error"],
        "no-array-constructor": ["error"],
        "no-extra-boolean-cast": ["error"],
        "no-constant-condition": ["error"],
        "no-dupe-class-members": ["error"],
        "no-unexpected-multiline": ["error"],
        "no-irregular-whitespace": ["error"],
        "no-empty-character-class": ["error"],
        "no-mixed-spaces-and-tabs": ["error"],
        "no-shadow-restricted-names": ["error"],
        "no-whitespace-before-property": ["error"],
        "no-unmodified-loop-condition": ["error"],
        "no-spaced-func": ["error"],
        "no-unused-labels": ["error"],
        "no-undefined": ["error"],
        "new-parens": ["error"],
        "require-yield": ["error"],
        "symbol-description": ["error"],
        "unicode-bom": ["error"],
        "yoda": ["error"],
        "no-confusing-arrow": ["off"],
        "no-empty-function": ["off"],
        "no-else-return": ["off"],
        "no-div-regex": ["off"],
        "no-continue": ["off"],
        "no-extra-parens": ["off"],
        "no-floating-decimal": ["off"],
        "no-global-assign": ["off"],
        "no-implicit-coercion": ["off"],
        "no-implicit-globals": ["off"],
        "no-implied-eval": ["off"],
        "no-inline-comments": ["off"],
        "no-inner-declarations": ["off"],
        "no-invalid-this": ["off"],
        "no-iterator": ["off"],
        "no-label-var": ["off"],
        "no-labels": ["off"],
        "no-lone-blocks": ["off"],
        "no-lonely-if": ["off"],
        "no-loop-func": ["off"],
        "no-magic-numbers": ["off"],
        "no-mixed-operators": ["off"],
        "no-mixed-requires": ["off"],
        "no-multi-spaces": ["off", {"ignoreEOLComments": true}],
        "no-multi-str": ["off"],
        "no-negated-condition": ["off"],
        "no-nested-ternary": ["off"],
        "no-new": ["off"],
        "no-new-func": ["off"],
        "no-new-object": ["off"],
        "no-new-require": ["off"],
        "no-new-wrappers": ["off"],
        "no-octal-escape": ["off"],
        "no-param-reassign": ["off"],
        "no-path-concat": ["off"],
        "no-plusplus": ["off", { "allowForLoopAfterthoughts": true }],
        "no-process-exit": ["off"],
        "no-proto": ["off"],
        "no-prototype-builtins": ["off"],
        "no-restricted-imports": ["off"],
        "no-restricted-modules": ["off"],
        "no-restricted-syntax": ["off"],
        "no-return-assign": ["off"],
        "no-script-url": ["off"],
        "no-self-compare": ["off"],
        "no-sequences": ["off"],
        "no-shadow": ["off"],
        "no-sync": ["off"],
        "no-tabs": ["off"],
        "no-ternary": ["off"],
        "no-trailing-spaces": ["off"],
        "no-underscore-dangle": ["off"],
        "no-unused-expressions": ["off"],
        "no-useless-call": ["off"],
        "no-useless-concat": ["off"],
        "no-useless-constructor": ["off"],
        "no-useless-escape": ["off"],
        "no-useless-rename": ["off"],
        "no-void": ["off"],
        "no-var": ["off"],
        "no-warning-comments": ["off"],
        "no-with": ["off"],
        "array-bracket-spacing": ["off"],
        "array-callback-return": ["off"],
        "arrow-body-style": ["off"],
        "arrow-parens": ["off"],
        "accessor-pairs": ["off"],
        "block-scoped-var": ["off"],
        "block-spacing": ["off"],
        "brace-style": ["off"],
        "callback-return": ["off"],
        "camelcase": ["off"],
        "class-methods-use-this": ["off"],
        "complexity": ["off"],
        "computed-property-spacing": ["off"],
        "consistent-return": ["off"],
        "consistent-this": ["off"],
        "dot-notation": ["off"],
        "eol-last": ["off"],
        "eqeqeq": ["off"],
        "func-names": ["off"],
        "func-style": ["off"],
        "generator-star-spacing": ["off"],
        "global-require": ["off"],
        "guard-for-in": ["off"],
        "handle-callback-err": ["off"],
        "id-blacklist": ["off"],
        "id-length": ["off"],
        "id-match": ["off"],
        "init-declarations": ["off"],
        "jsx-quotes": ["off"],
        "keyword-spacing": ["off"],
        "line-comment-position": ["off"],
        "lines-around-comment": ["off"],
        "lines-around-directive": ["off"],
        "max-depth": ["off"],
        "max-len": ["off"],
        "max-lines": ["off"],
        "max-nested-callbacks": ["off"],
        "max-params": ["off"],
        "max-statements": ["off"],
        "max-statements-per-line": ["off"],
        "multiline-ternary": ["off"],
        "new-cap": ["off"],
        "newline-after-var": ["off"],
        "newline-before-return": ["off"],
        "newline-per-chained-call": ["off"],
        "object-curly-newline": ["off"],
        "object-curly-spacing": ["off", "never"],
        "object-property-newline": ["off"],
        "object-shorthand": ["off"],
        "one-var": ["off"],
        "one-var-declaration-per-line": ["off"],
        "operator-assignment": ["off"],
        "padded-blocks": ["off"],
        "prefer-arrow-callback": ["off"],
        "prefer-const": ["off"],
        "prefer-reflect": ["off"],
        "prefer-rest-params": ["off"],
        "prefer-spread": ["off"],
        "prefer-template": ["off"],
        "quote-props": ["off"],
        "rest-spread-spacing": ["off"],
        "sort-keys": ["off"],
        "sort-imports": ["off"],
        "sort-vars": ["off"],
        "space-before-blocks": ["off"],
        "space-before-function-paren": ["off"],
        "space-in-parens": ["off"],
        "space-infix-ops": ["off"],
        "space-unary-ops": ["off"],
        "spaced-comment": ["off"],
        "template-curly-spacing": ["off"],
        "vars-on-top": ["off"],
        "wrap-iife": ["off"],
        "wrap-regex": ["off"],
        "no-template-curly-in-string": ["off"],
        "yield-star-spacing": ["off"]
    }
}
